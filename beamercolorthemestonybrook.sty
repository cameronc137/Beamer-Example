\ProvidesPackageRCS $Header: /cvsroot/latex-beamer/latex-beamer/themes/color/beamercolorthemestonybrook.sty,v 1.4 2007/01/28 20:48:24 tantau Exp $

\definecolor{sbured}{RGB}{153,0,0}
\definecolor{coolgray}{RGB}{75,75,75}
\definecolor{black}{RGB}{0,0,0}
\definecolor{beige}{RGB}{210,194,149}
\definecolor{darkbeige}{RGB}{179,153,93}
\definecolor{darksbured}{RGB}{107,0,13}
\definecolor{lightsbured}{RGB}{213,32,39}
\definecolor{darkpurple}{RGB}{83,40,79}
\definecolor{darkcyan}{RGB}{0,124,146}
\definecolor{skyblue}{RGB}{0,152,219}
\definecolor{treegreen}{RGB}{0,155,118}
\definecolor{darkorange}{RGB}{168,101,12}
\definecolor{beigegray}{RGB}{87,87,87}
\definecolor{boxgray}{RGB}{235,235,235}
\definecolor{footergray}{RGB}{190,190,190}
\definecolor{lightgray}{RGB}{220,220,220}



\mode<presentation>

\setbeamercolor*{palette primary}{use=structure,fg=white,bg= sbured}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg= coolgray}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg= darksbured}
\setbeamercolor*{palette quaternary}{fg=white,bg= darkbeige}

\setbeamercolor*{sidebar}{use=structure,bg= beige}
\setbeamercolor*{footer}{use=structure,bg= footergray,fg=darksbured}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor*{foot line}{parent=palette secondary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor{itemize item}{fg=sbured}
\setbeamercolor{itemize subitem}{fg=sbured}
\setbeamercolor{itemize subsubitem}{fg=sbured}
\setbeamercolor{enumerate item}{fg=sbured}
\setbeamercolor{enumerate subitem}{fg=sbured}
\setbeamercolor{enumerate subsubitem}{fg=sbured}
\setbeamercolor{description item}{fg=sbured}

\setbeamertemplate{bibliography item}[text]
\setbeamertemplate{frametitle continuation}[from second]
\setbeamercolor{bibliography entry title}{fg=black}
\setbeamercolor{bibliography entry author}{fg=black}
\setbeamercolor*{bibliography entry location}{fg=black}
\setbeamercolor*{bibliography entry note}{fg=black}


\mode
<all>
